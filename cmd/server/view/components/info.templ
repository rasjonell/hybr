package components

templ Info(title string, info []string) {
	<div class="bg-gray-900 p-4 border-2 border-green-400 col-span-full">
		<h3 class="text-2xl mb-2">Active Services</h3>
		<ul class="list-disc list-inside">
			for _, item := range info {
				<li>
					{ item }
				</li>
			}
		</ul>
	</div>
}
