[
  {
    "name": "rss",
    "description": "Tiny Tiny RSS - Self-hosted news feed aggregator",
    "subDomain": true,
    "variables": {
      ".env.templ": [
        {
          "name": "PORT",
          "default": "8280",
          "description": "HTTP Port"
        },
        {
          "default": "Password",
          "name": "ADMIN_USER_PASS",
          "description": "Admin user password for TT-RSS"
        }
      ]
    },
    "templates": [
      "docker-compose.yml.templ",
      ".env.templ"
    ]
  },
  {
    "name": "nextcloud",
    "description": "Nextcloud - Personal Cloud Provider",
    "subDomain": true,
    "variables": {
      ".env.templ": [
        {
          "name": "PORT",
          "default": "8281",
          "description": "HTTP Port"
        },
        {
          "name": "MYSQL_ROOT_PASSWORD",
          "default": "nextcloud",
          "description": "MariaDB Root Password"
        },
        {
          "name": "MYSQL_USER",
          "default": "nextcloud",
          "description": "MariaDB User Name"
        },
        {
          "name": "MYSQL_PASSWORD",
          "default": "nextcloud",
          "description": "MariaDB User Password"
        },
        {
          "name": "MYSQL_DATABASE",
          "default": "nextcloud",
          "description": "MariaDB Databse Name"
        }
      ]
    },
    "templates": [
      "docker-compose.yml.templ",
      ".env.templ"
    ]
  }
]
